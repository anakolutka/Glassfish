<sun-ejb-jar>
	<enterprise-beans>
		<name>SayHelloJAR</name>
		<ejb>
			<ejb-name>SayHelloEJB</ejb-name>
			<jndi-name>SayHelloEJB</jndi-name>
			<webservice-endpoint>
				<port-component-name>SayHelloPort</port-component-name>
