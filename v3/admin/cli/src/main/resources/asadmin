[header]
comment1=Executable script to invoke administrative commands for the server
[source]
config-file=${script.home}/config/asenv
[env.vars]
AS_INSTALL=${script.home}
AS_LIBS=#{AS_INSTALL}/modules
# no [shell.vars]
[jvm]
main.class=com.sun.enterprise.admin.cli.AsadminMain
[classpath]
CP1=#{AS_LIBS}/admin-cli-${glassfish.version}.jar
CP2=#{AS_LIBS}/cli-framework-${glassfish.version}.jar
CP3=#{AS_LIBS}/glassfish-${glassfish.version}.jar
CP4=#{AS_LIBS}/universal-${glassfish.version}.jar
[java.system.properties]
com.sun.aas.installRoot=#{AS_INSTALL}