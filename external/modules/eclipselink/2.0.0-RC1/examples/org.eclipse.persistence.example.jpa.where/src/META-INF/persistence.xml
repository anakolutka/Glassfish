<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="geonames">
		<mapping-file>META-INF/orm.xml</mapping-file>
		<class>example.where.model.geonames.TimeZone</class>
		<class>example.where.model.geonames.Point</class>
		<class>example.where.model.geonames.Language</class>
		<class>example.where.model.geonames.Feature</class>
		<class>example.where.model.geonames.CountryLanguage</class>
		<class>example.where.model.geonames.Country</class>
		<class>example.where.model.geonames.Continent</class>
		<class>example.where.model.geonames.AdminDivision</class>
		<class>example.where.model.User</class>
		<class>example.where.model.Location</class>
		<class>example.where.model.Group</class>
	</persistence-unit>
</persistence>