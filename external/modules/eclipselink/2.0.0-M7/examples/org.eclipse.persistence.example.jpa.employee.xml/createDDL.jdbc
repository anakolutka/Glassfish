CREATE TABLE PHONE (TYPE VARCHAR2(255) NOT NULL, EMP_ID NUMBER(10) NOT NULL, AREA_CODE VARCHAR2(255) NULL, P_NUMBER VARCHAR2(255) NULL, PRIMARY KEY (TYPE, EMP_ID))
CREATE TABLE SALARY (EMP_ID NUMBER(10) NOT NULL, SALARY NUMBER(19,4) NULL, PRIMARY KEY (EMP_ID))
CREATE TABLE EMPLOYEE (EMP_ID NUMBER(10) NOT NULL, L_NAME VARCHAR2(255) NULL, END_TIME TIMESTAMP NULL, VERSION NUMBER(19) NULL, START_TIME TIMESTAMP NULL, GENDER VARCHAR2(255) NULL, F_NAME VARCHAR2(255) NULL, START_DATE DATE NULL, END_DATE DATE NULL, MANAGER_ID NUMBER(10) NULL, ADDR_ID NUMBER(10) NULL, PRIMARY KEY (EMP_ID))
CREATE TABLE PROJ_EMP (EMP_ID NUMBER(10) NOT NULL, PROJ_ID NUMBER(10) NOT NULL, PRIMARY KEY (EMP_ID, PROJ_ID))
CREATE TABLE ADDRESS (ADDRESS_ID NUMBER(10) NOT NULL, P_CODE VARCHAR2(255) NULL, STREET VARCHAR2(255) NULL, PROVINCE VARCHAR2(255) NULL, COUNTRY VARCHAR2(255) NULL, CITY VARCHAR2(255) NULL, PRIMARY KEY (ADDRESS_ID))
CREATE TABLE PROJECT (PROJ_ID NUMBER(10) NOT NULL, PROJ_TYPE VARCHAR2(31) NULL, DESCRIP VARCHAR2(255) NULL, PROJ_NAME VARCHAR2(255) NULL, VERSION NUMBER(19) NULL, LEADER_ID NUMBER(10) NULL, PRIMARY KEY (PROJ_ID))
CREATE TABLE RESPONS (EMP_ID NUMBER(10) NOT NULL, RESPON_DESC VARCHAR2(255) NULL)
CREATE TABLE LPROJECT (PROJ_ID NUMBER(10) NOT NULL, MILESTONE TIMESTAMP NULL, BUDGET NUMBER(19,4) NULL, PRIMARY KEY (PROJ_ID))
ALTER TABLE PHONE ADD CONSTRAINT FK_PHONE_EMP_ID FOREIGN KEY (EMP_ID) REFERENCES EMPLOYEE (EMP_ID)
