# Use is subject to license terms.
#

AS_INSTALL=`dirname $0`/..
AS_INSTALL_LIB=$AS_INSTALL/modules
ANT_LIB=$AS_INSTALL_LIB/ant-@VERSION@.jar
SERVLET_API=$AS_INSTALL_LIB/@SERVLET_API@
JSP_API=$AS_INSTALL_LIB/@JSP_API@
JSP_IMPL=$AS_INSTALL_LIB/@JSP_IMPL@
EL_IMPL=$AS_INSTALL_LIB/@EL_IMPL@
JSTL_API=$AS_INSTALL_LIB/@JSTL_API@
JSTL_IMPL=$AS_INSTALL_LIB/@JSTL_IMPL@
JSF_API=$AS_INSTALL_LIB/@JSF_API@
JSF_IMPL=$AS_INSTALL_LIB/@JSF_IMPL@

java -cp "$SERVLET_API:$JSP_API:$JSTL_API:$JSF_API:$ANT_LIB:$EL_IMPL:$JSP_IMPL" org.apache.jasper.JspC -classpath "$JSTL_IMPL:$JSF_IMPL" "$@"

