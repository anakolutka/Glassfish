 <sun:treeNode id="Jmail"
        text="$resource{i18n.tree.jndi}"
        url="/full/jndi.jsf"
        imageURL="/resource/full/images/jndi.gif"
        >
            
        <dynamicTreeNode id="customResources"
        treeAdaptorClass="org.glassfish.admingui.common.tree.MBeanTreeAdaptor"
        objectName="amx:pp=/domain,type=resources"
        useV3AMX="true"
        amxChildType="custom-resource"
        methodName=""
        attributeName="name"
        childImageURL="/resource/full/images/jndi.gif"
        childExpanded="$boolean{false}"
        childURL="/full/customResourceEdit.jsf?name=#{$this{valueBinding}.text}"
        text="$resource{i18n.tree.customResources}"
        url="/full/customResources.jsf"
        >
        <!facet image>
            <sun:iconHyperlink id="abc" icon="TREE_FOLDER" url="/full/customResources.jsf"  border="$int{0}" immediate="$boolean{true}" />
        </facet>
        </dynamicTreeNode>                  
        <dynamicTreeNode id="externalResources"
        treeAdaptorClass="org.glassfish.admingui.common.tree.MBeanTreeAdaptor"
        objectName="amx:pp=/domain,type=resources"
        useV3AMX="true"
        amxChildType="external-jndi-resource"
        methodName=""
        attributeName="name"
        childImageURL="/resource/full/images/jndi.gif"
        childExpanded="$boolean{false}"
        childURL="/full/externalResourceEdit.jsf?name=#{$this{valueBinding}.text}"
        text="$resource{i18n.tree.externalResources}"
        url="/full/externalResources.jsf"
        >
        <!facet image>
            <sun:iconHyperlink id="abc" icon="TREE_FOLDER" url="/full/externalResources.jsf"  border="$int{0}" immediate="$boolean{true}" />
        </facet>
        </dynamicTreeNode>  
 </sun:treeNode> 
    <dynamicTreeNode id="mailResources"
        treeAdaptorClass="org.glassfish.admingui.common.tree.MBeanTreeAdaptor"
        objectName="amx:pp=/domain,type=resources"
        useV3AMX="true"
        amxChildType="mail-resource"
        methodName=""
        attributeName="name"
        childImageURL="/resource/full/images/javaMailSession.gif"
        childExpanded="$boolean{false}"
        childURL="/full/mailResourceEdit.jsf?name=#{$this{valueBinding}.text}"
        text="$resource{i18n.tree.javaMailSessions}"
        url="/full/mailResources.jsf"
        >
        <!facet image>
            <sun:iconHyperlink id="abc" icon="TREE_FOLDER" url="/full/mailResources.jsf"  border="$int{0}" immediate="$boolean{true}" />
        </facet>
    </dynamicTreeNode>  
