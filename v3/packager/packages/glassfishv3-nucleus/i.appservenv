PATH=/usr/bin:/usr/sbin:/bin

while read src dest
do
	sed \
		-e "s,\%CONFIG_HOME\%,$CLIENT_BASEDIR/appserver/config," \
                -e "s,\$BASEDIR,$CLIENT_BASEDIR," \
		-e "s,//,/,g" \
		< $src > $dest
done
exit 0
