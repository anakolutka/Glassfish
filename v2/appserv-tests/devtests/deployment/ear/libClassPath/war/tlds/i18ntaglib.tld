<!DOCTYPE taglib PUBLIC '-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN' 'http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd'>

<taglib>
<tlibversion>1.0</tlibversion>
<jspversion>1.1</jspversion>
<shortname>i18n</shortname>
<info>taglib for localized jsps</info>

<tag>
<name>message</name>
<tagclass>samples.i18n.simple.i18ntag.MessageTag</tagclass>
<bodycontent>JSP</bodycontent>
<info>Get Message from Bundle</info>

<attribute>
<name>key</name>
<required>true</required>
<rtexprvalue>false</rtexprvalue>
</attribute>

<attribute>
<name>name</name>
<required>true</required>
<rtexprvalue>false</rtexprvalue>
</attribute>

<attribute>
<name>language</name>
<required>true</required>
<rtexprvalue>false</rtexprvalue>
</attribute>

<attribute>
<name>country</name>
<required>false</required>
<rtexprvalue>false</rtexprvalue>
</attribute>

<attribute>
<name>variant</name>
<required>false</required>
<rtexprvalue>false</rtexprvalue>
</attribute>

</tag>

</taglib>
