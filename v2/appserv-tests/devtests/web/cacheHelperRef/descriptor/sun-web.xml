<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 8.1 Servlet 2.4//EN" "http://www.sun.com/software/sunone/appserver/dtds/sun-web-app_2_4-1.dtd">

<sun-web-app>
  <cache enabled="true">
    <cache-helper name="myKey" class-name="helpers.MyCacheHelper"/>
    <cache-mapping>
      <servlet-name>TestServlet</servlet-name>
      <cache-helper-ref>myKey</cache-helper-ref>
    </cache-mapping>
  </cache>
</sun-web-app>

