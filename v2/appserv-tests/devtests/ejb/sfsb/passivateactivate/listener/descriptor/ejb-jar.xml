<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <display-name>Ejb1</display-name>
  <enterprise-beans>

    <session>
      <display-name>SFSBEJB</display-name>
      <ejb-name>SFSBEJB</ejb-name>
      <home>com.sun.s1asdev.ejb.ejb30.sfsb.lifecycle.ejb.SFSBHome</home>
      <remote>com.sun.s1asdev.ejb.ejb30.sfsb.lifecycle.ejb.SFSB</remote>
      <local-home>com.sun.s1asdev.ejb.ejb30.sfsb.lifecycle.ejb.SFSBLocalHome</local-home>
      <local>com.sun.s1asdev.ejb.ejb30.sfsb.lifecycle.ejb.SFSBLocal</local>
      <ejb-class>com.sun.s1asdev.ejb.ejb30.sfsb.lifecycle.ejb.SFSBEJB</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Bean</transaction-type>
      <env-entry>
        <env-entry-name>TagValue</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>MyTagValue</env-entry-value>
      </env-entry>
      <security-identity>
        <use-caller-identity/>
      </security-identity>
    </session>

  </enterprise-beans>

</ejb-jar>
