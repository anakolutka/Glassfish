<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE jdo PUBLIC "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 1.0//EN" "http://java.sun.com/dtd/jdo_1_0.dtd">
<jdo>
<!--
This file contains the schema information when an implementation
has application identity.

Please note, this file is changed wrt. the JDO apache version:
Added field elements for each declared field.
-->
    <package name="com.sun.org.apache.jdo.tck.pc.company">

        <class name="Address"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Address$Oid">
            <field name="addrid" primary-key="true"/>
            <field name="city"/>
            <field name="state"/>
            <field name="street"/>
            <field name="zipcode"/>
            <field name="country"/>
        </class>

        <class name="Company"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Company$Oid">
            <field name="companyid" primary-key="true"/>
            <field name="founded"/>
            <field name="name"/>
            <field name="departments" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Department"/>
            </field>
            <field name="address" embedded="true"/>
        </class>

        <class name="DentalInsurance"
            identity-type="application"
            persistence-capable-superclass="com.sun.org.apache.jdo.tck.pc.company.Insurance">
            <field name="lifetimeOrthoBenefit"/>
        </class>

        <class name="Department"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Department$Oid">
            <field name="deptid" primary-key="true"/>
            <field name="name"/>
            <field name="company"/>
            <field name="employeeOfTheMonth"/>
            <field name="employees" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
            <field name="fundedEmps" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
        </class>

        <class name="Employee"
            identity-type="application"
            persistence-capable-superclass="com.sun.org.apache.jdo.tck.pc.company.Person">
            <field name="hiredate"/>
            <field name="weeklyhours"/>
            <field name="dentalInsurance"/>
            <field name="department"/>
            <field name="fundingDept"/>
            <field name="hradvisor"/>
            <field name="manager"/>
            <field name="medicalInsurance"/>
            <field name="mentor"/>
            <field name="protege"/>
            <field name="reviewedProjects" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Project"/>
            </field>
            <field name="projects" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Project"/>
            </field>
            <field name="team" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
            <field name="hradvisees" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
        </class>

        <class name="FullTimeEmployee"
            identity-type="application"
            persistence-capable-superclass="com.sun.org.apache.jdo.tck.pc.company.Employee">
            <field name="salary"/>
        </class>

        <class name="Insurance"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Insurance$Oid">
            <field name="insid" primary-key="true"/>
            <field name="carrier"/>
            <field name="employee"/>
        </class>

        <class name="MedicalInsurance"
            identity-type="application"
            persistence-capable-superclass="com.sun.org.apache.jdo.tck.pc.company.Insurance">
            <field name="planType"/>
        </class>

        <class name="PartTimeEmployee"
            identity-type="application"
            persistence-capable-superclass="com.sun.org.apache.jdo.tck.pc.company.Employee">
            <field name="wage"/>
        </class>

        <class name="Person"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Person$Oid">
            <field name="personid" primary-key="true"/>
            <field name="middlename" default-fetch-group="false"/>
            <field name="address" embedded="true"/>
            <field name="birthdate"/>
            <field name="firstname"/>
            <field name="lastname"/>
	    <field name="phoneNumbers" persistence-modifier="persistent">
                <map key-type="String" value-type="String"/>
            </field>
        </class>

        <class name="Project"
            identity-type="application"
            objectid-class="com.sun.org.apache.jdo.tck.pc.company.Project$Oid">
            <field name="projid" primary-key="true"/>
            <field name="budget"/>
            <field name="name"/>
            <field name="reviewers" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
            <field name="members" persistence-modifier="persistent">
                <collection element-type="com.sun.org.apache.jdo.tck.pc.company.Employee"/>
            </field>
        </class>
    </package>
</jdo>
