connect test-mbs

# register our two test MBeans
register-mbean --class com.sun.cli.jmxcmd.test.mbeans.CLISupportTestee jmxcmd.test:name=cli-support-testee
register-mbean --class com.sun.cli.jmxcmd.test.mbeans.CLISupportSimpleTestee jmxcmd.test:name=cli-support-simple-testee
